<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>User</th>
      <th>Date tracked</th>
      <th>Target</th>
      <th>Consumed calories</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_calories_data.each do |user_calories_datum| %>
      <tr>
        <td><%= user_calories_datum.user_info_id %></td>
        <td><%= user_calories_datum.date_tracked %></td>
        <td><%= user_calories_datum.target %></td>
        <td><%= user_calories_datum.consumed_calories %></td>
        <td><%= link_to 'Show', user_calories_datum %></td>
        <td><%= link_to 'Edit', edit_user_calories_datum_path(user_calories_datum) %></td>
        <td><%= link_to 'Destroy', user_calories_datum, method: :delete, :remote => true, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

