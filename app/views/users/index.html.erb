
<div class="panel">
  <div class="panel-heading">
    <h3 class="panel-title">Welcome To CalorieMeter</h3>
  </div>
<%= form_tag sessions_path do %>
  <p>
    <div class="label label-default">
    <%= label_tag :userid %><br />
    <%= text_field_tag :email, params[:email] %>
    </div>
  </p>
  <p>
    <div class="label label-default">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
    </div>
  </p>

  <p button type="button" class="btn btn-success"><%= submit_tag "Log in", :remote => true %></button></p>
<% end %>
</div>






